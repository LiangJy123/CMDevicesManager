<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Navigation Menu -->
    <sys:String x:Key="HomeText" xmlns:sys="clr-namespace:System;assembly=mscorlib">首页</sys:String>
    <sys:String x:Key="DeviceText" xmlns:sys="clr-namespace:System;assembly=mscorlib">设备</sys:String>
    <sys:String x:Key="SettingsText" xmlns:sys="clr-namespace:System;assembly=mscorlib">设置</sys:String>
    
    <!-- Main Window -->
    <sys:String x:Key="AppTitle" xmlns:sys="clr-namespace:System;assembly=mscorlib">Cooler Master</sys:String>
    
    <!-- Pages -->
    <sys:String x:Key="HomePageTitle" xmlns:sys="clr-namespace:System;assembly=mscorlib">主页</sys:String>
    <sys:String x:Key="DevicePageTitle" xmlns:sys="clr-namespace:System;assembly=mscorlib">设备管理</sys:String>
    <sys:String x:Key="SettingsPageTitle" xmlns:sys="clr-namespace:System;assembly=mscorlib">设置</sys:String>
    
    <!-- Settings Page -->
    <sys:String x:Key="LanguageLabel" xmlns:sys="clr-namespace:System;assembly=mscorlib">语言设置</sys:String>
    <sys:String x:Key="ChineseLanguage" xmlns:sys="clr-namespace:System;assembly=mscorlib">简体中文</sys:String>
    <sys:String x:Key="TraditionalChineseLanguage" xmlns:sys="clr-namespace:System;assembly=mscorlib">繁体中文</sys:String>
    <sys:String x:Key="EnglishLanguage" xmlns:sys="clr-namespace:System;assembly=mscorlib">English</sys:String>
    <sys:String x:Key="FontLabel" xmlns:sys="clr-namespace:System;assembly=mscorlib">字体设置</sys:String>
    <sys:String x:Key="DefaultFont" xmlns:sys="clr-namespace:System;assembly=mscorlib">系统默认</sys:String>
    <sys:String x:Key="NotoSansFont" xmlns:sys="clr-namespace:System;assembly=mscorlib">Noto Sans</sys:String>
    <sys:String x:Key="NotoSansCJKFont" xmlns:sys="clr-namespace:System;assembly=mscorlib">Noto Sans CJK</sys:String>
    <sys:String x:Key="RubikFont" xmlns:sys="clr-namespace:System;assembly=mscorlib">Rubik</sys:String>
    <sys:String x:Key="ApplySettings" xmlns:sys="clr-namespace:System;assembly=mscorlib">应用设置</sys:String>

    <!-- Update -->
    <sys:String x:Key="UpdateLabel" xmlns:sys="clr-namespace:System;assembly=mscorlib">更新</sys:String>
    <sys:String x:Key="CheckUpdate" xmlns:sys="clr-namespace:System;assembly=mscorlib">检查更新</sys:String>
    
    <!-- Common -->
    <sys:String x:Key="HelloText" xmlns:sys="clr-namespace:System;assembly=mscorlib">你好</sys:String>
    <sys:String x:Key="WelcomeText" xmlns:sys="clr-namespace:System;assembly=mscorlib">欢迎使用 Cooler Master 设备管理器</sys:String>
    
    <!-- Dialog Buttons -->
    <sys:String x:Key="OkButton" xmlns:sys="clr-namespace:System;assembly=mscorlib">确定</sys:String>
    <sys:String x:Key="CancelButton" xmlns:sys="clr-namespace:System;assembly=mscorlib">取消</sys:String>
    <sys:String x:Key="LoadButton" xmlns:sys="clr-namespace:System;assembly=mscorlib">加载</sys:String>
    
    <!-- DeviceConfig Page -->
    <sys:String x:Key="ConfigNameTitle" xmlns:sys="clr-namespace:System;assembly=mscorlib">配置名称</sys:String>
    <sys:String x:Key="ConfigNamePrompt" xmlns:sys="clr-namespace:System;assembly=mscorlib">请输入配置名称：</sys:String>
    <sys:String x:Key="SelectConfigTitle" xmlns:sys="clr-namespace:System;assembly=mscorlib">选择配置</sys:String>
    <sys:String x:Key="ConfigSaved" xmlns:sys="clr-namespace:System;assembly=mscorlib">配置已保存</sys:String>
    <sys:String x:Key="SaveSuccessful" xmlns:sys="clr-namespace:System;assembly=mscorlib">保存成功</sys:String>
    <sys:String x:Key="ConfigLoaded" xmlns:sys="clr-namespace:System;assembly=mscorlib">配置已加载</sys:String>
    <sys:String x:Key="LoadSuccessful" xmlns:sys="clr-namespace:System;assembly=mscorlib">加载成功</sys:String>
    <sys:String x:Key="LoadConfigFailed" xmlns:sys="clr-namespace:System;assembly=mscorlib">加载配置失败</sys:String>
    <sys:String x:Key="Error" xmlns:sys="clr-namespace:System;assembly=mscorlib">错误</sys:String>
    <sys:String x:Key="UnsavedConfig" xmlns:sys="clr-namespace:System;assembly=mscorlib">未保存的配置</sys:String>
    <sys:String x:Key="SampleText" xmlns:sys="clr-namespace:System;assembly=mscorlib">示例文本</sys:String>
    
    <!-- Status Information -->
    <sys:String x:Key="None" xmlns:sys="clr-namespace:System;assembly=mscorlib">无</sys:String>
    <sys:String x:Key="CpuUsage" xmlns:sys="clr-namespace:System;assembly=mscorlib">CPU 使用率</sys:String>
    <sys:String x:Key="GpuUsage" xmlns:sys="clr-namespace:System;assembly=mscorlib">GPU 使用率</sys:String>
    <sys:String x:Key="DateTime" xmlns:sys="clr-namespace:System;assembly=mscorlib">日期时间</sys:String>
    <sys:String x:Key="LiveText" xmlns:sys="clr-namespace:System;assembly=mscorlib">实时文本</sys:String>
    
    <!-- System Tray -->
    <sys:String x:Key="TrayTooltip" xmlns:sys="clr-namespace:System;assembly=mscorlib">CMDevicesManager - 硬件监控工具</sys:String>
    <sys:String x:Key="ShowMainWindow" xmlns:sys="clr-namespace:System;assembly=mscorlib">显示主窗口</sys:String>
    <sys:String x:Key="ExitApplication" xmlns:sys="clr-namespace:System;assembly=mscorlib">退出应用程序</sys:String>
    <sys:String x:Key="AppRunningInTray" xmlns:sys="clr-namespace:System;assembly=mscorlib">应用程序正在系统托盘中运行。双击打开或右键查看选项。</sys:String>
    <sys:String x:Key="AppMinimizedToTray" xmlns:sys="clr-namespace:System;assembly=mscorlib">应用程序已最小化到系统托盘。双击托盘图标以恢复。</sys:String>
    <sys:String x:Key="CloseConfirmation" xmlns:sys="clr-namespace:System;assembly=mscorlib">关闭确认</sys:String>
    <sys:String x:Key="CloseConfirmationMessage" xmlns:sys="clr-namespace:System;assembly=mscorlib">您是否要最小化到系统托盘或退出应用程序？

点击"是"最小化到托盘（应用程序继续运行）
点击"否"完全退出
点击"取消"返回应用程序</sys:String>
    
    <!-- Additional Messages -->
    <sys:String x:Key="NoConfigFilesFound" xmlns:sys="clr-namespace:System;assembly=mscorlib">没有找到任何配置文件</sys:String>
    <sys:String x:Key="NoValidConfigFilesFound" xmlns:sys="clr-namespace:System;assembly=mscorlib">没有找到有效的配置文件</sys:String>
    <sys:String x:Key="Notice" xmlns:sys="clr-namespace:System;assembly=mscorlib">提示</sys:String>
    <sys:String x:Key="OpenSettingsPlaceholder" xmlns:sys="clr-namespace:System;assembly=mscorlib">这里打开设置窗口</sys:String>
    <sys:String x:Key="SaveAsButton" xmlns:sys="clr-namespace:System;assembly=mscorlib">另存为</sys:String>
    
    <!-- MessageBox Messages -->
    <sys:String x:Key="Warning" xmlns:sys="clr-namespace:System;assembly=mscorlib">警告</sys:String>
    <sys:String x:Key="Info" xmlns:sys="clr-namespace:System;assembly=mscorlib">信息</sys:String>
    <sys:String x:Key="Success" xmlns:sys="clr-namespace:System;assembly=mscorlib">成功</sys:String>
    <sys:String x:Key="HomePageLoadFailed" xmlns:sys="clr-namespace:System;assembly=mscorlib">警告：无法加载主页。某些功能可能无法正常工作。</sys:String>
    <sys:String x:Key="CanvasNotReady" xmlns:sys="clr-namespace:System;assembly=mscorlib">画布尚未准备好。</sys:String>
    <sys:String x:Key="SaveFailed" xmlns:sys="clr-namespace:System;assembly=mscorlib">保存失败：{0}</sys:String>
    <sys:String x:Key="ScreenshotSaved" xmlns:sys="clr-namespace:System;assembly=mscorlib">截图已保存：
{0}</sys:String>
    <sys:String x:Key="ScreenshotFailed" xmlns:sys="clr-namespace:System;assembly=mscorlib">截图失败：{0}</sys:String>
    <sys:String x:Key="ConfigSaveFailed" xmlns:sys="clr-namespace:System;assembly=mscorlib">保存配置失败：{0}</sys:String>
    <sys:String x:Key="SaveAsFailed" xmlns:sys="clr-namespace:System;assembly=mscorlib">另存为失败：{0}</sys:String>
    <sys:String x:Key="NameConflict" xmlns:sys="clr-namespace:System;assembly=mscorlib">命名冲突</sys:String>
    <sys:String x:Key="NameAlreadyExists" xmlns:sys="clr-namespace:System;assembly=mscorlib">名称 "{0}" 仍已存在，请重新输入。</sys:String>
    <sys:String x:Key="FileError" xmlns:sys="clr-namespace:System;assembly=mscorlib">文件错误</sys:String>
    <sys:String x:Key="FileNotExist" xmlns:sys="clr-namespace:System;assembly=mscorlib">所选文件不存在。</sys:String>
    <sys:String x:Key="FileSizeValidationFailed" xmlns:sys="clr-namespace:System;assembly=mscorlib">文件大小验证失败：{0}</sys:String>
    <sys:String x:Key="ReadVideoInfoFailed" xmlns:sys="clr-namespace:System;assembly=mscorlib">读取视频信息失败</sys:String>
    <sys:String x:Key="ExtractVideoFramesFailed" xmlns:sys="clr-namespace:System;assembly=mscorlib">提取视频帧失败</sys:String>
    <sys:String x:Key="LoadMp4Failed" xmlns:sys="clr-namespace:System;assembly=mscorlib">加载MP4失败：{0}</sys:String>
    <sys:String x:Key="ConfigurationLoaded" xmlns:sys="clr-namespace:System;assembly=mscorlib">配置已加载：{0}</sys:String>
    <sys:String x:Key="AddConfigFailed" xmlns:sys="clr-namespace:System;assembly=mscorlib">添加配置失败：{0}</sys:String>
    <sys:String x:Key="LoadConfigurationFailed" xmlns:sys="clr-namespace:System;assembly=mscorlib">加载配置失败：{0}</sys:String>
    <sys:String x:Key="InitializationFailed" xmlns:sys="clr-namespace:System;assembly=mscorlib">初始化失败：{0}</sys:String>
    <sys:String x:Key="NoConfigFolderFound" xmlns:sys="clr-namespace:System;assembly=mscorlib">未找到配置文件夹。</sys:String>
    <sys:String x:Key="NoValidConfigFilesLoaded" xmlns:sys="clr-namespace:System;assembly=mscorlib">无法加载有效的配置文件。</sys:String>
    <sys:String x:Key="UnableToOpenWebsite" xmlns:sys="clr-namespace:System;assembly=mscorlib">无法打开网站。</sys:String>
    <sys:String x:Key="CurrentCanvasNotReady" xmlns:sys="clr-namespace:System;assembly=mscorlib">当前画布尚未准备好。</sys:String>
    <sys:String x:Key="FileSaved" xmlns:sys="clr-namespace:System;assembly=mscorlib">已保存：
{0}</sys:String>
</ResourceDictionary>